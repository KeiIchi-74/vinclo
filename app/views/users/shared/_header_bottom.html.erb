<div class='header-bottom-container'>
  <div class='header-bottom-selector-container'>
    <div class='header-bottom-selector'>
      <a class='header-bottom-selector-a fs-14 fw-600'>
        <i class="fas fa-home"></i>
        セレクター1
      </a>
    </div>
  </div>
  <div class='header-bottom-button'>
  <% if user_signed_in? %>
    <div class='logout-button fs-14'>
      <%= link_to 'ログアウト', destroy_user_session_path, class: 'logout-button-a', method: :delete %>
    </div>
  <% else %>
    <div class='register-button'>
      <%= link_to '新規会員登録', new_user_registration_path, class: 'register-button-a fs-14' %>
    </div>
    <div class='login-button'>
      <%= link_to 'ログイン', new_user_session_path, class: 'login-button-a fs-14' %>
    </div>
  <% end %>
  </div>
</div>